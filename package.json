    "@angular/platform-browser-dynamic": "^9.1.2",
    "@angular/router": "^9.1.2",
    "bootstrap": "^4.4.1",
    "codemirror": "^5.52.2",
    "copyfiles": "^2.2.0",
    "core-js": "^3.6.5",
    "diff-match-patch": "^1.0.4",
