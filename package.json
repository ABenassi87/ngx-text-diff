{
  "name": "ngx-text-diff-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build --configuration production",
    "build_report": "ng build --configuration production --stats-json",
    "test": "echo 1",
    "lint": "ng lint",
    "postinstall": "ngcc",
    "e2e": "ng e2e",
    "build_lib": "ng build ngx-text-diff --configuration production",
    "copy-license": "copyfiles ./projects/ngx-text-diff/LICENSE ./dist/ngx-text-diff",
    "copy-readme": "copyfiles ./projects/ngx-text-diff/README.md ./dist/ngx-text-diff",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "cd dist/ngx-text-diff && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.1.2",
    "@angular/cdk": "^13.1.2",
    "@angular/common": "^13.1.2",
    "@angular/compiler": "^13.1.2",
    "@angular/core": "^13.1.2",
    "@angular/forms": "^13.1.2",
    "@angular/platform-browser": "^13.1.2",
    "@angular/platform-browser-dynamic": "^13.1.2",
    "@angular/router": "^13.1.2",
    "bootstrap": "^5.1.3",
    "codemirror": "^5.65.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.20.3",
    "diff-match-patch": "^1.0.5",
    "express": "^4.17.2",
    "path": "^0.12.7",
    "rxjs": "~6.5.5",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.3",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "^13.1.3",
    "@angular/compiler-cli": "^13.1.2",
    "@angular/language-service": "^13.1.2",
    "@types/codemirror": "5.60.5",
    "@types/diff-match-patch": "^1.0.32",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^17.0.10",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.11",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.1.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.1.3",
    "prettier": "^2.5.1",
    "protractor": "~7.0.0",
    "ts-node": "^10.4.0",
    "tslint": "~6.1.3",
    "typescript": "^4.5.4",
    "webpack-bundle-analyzer": "^4.5.0"
  }
