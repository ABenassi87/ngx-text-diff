  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build --prod",
    "build_report": "ng build --prod --stats-json",
    "test": "echo 1",
    "lint": "ng lint",
    "postinstall": "ngcc",
    "e2e": "ng e2e",
    "build_lib": "ng build ngx-text-diff --prod",
    "copy-license": "copyfiles ./projects/ngx-text-diff/LICENSE ./dist/ngx-text-diff",
